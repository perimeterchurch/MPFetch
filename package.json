{
    "name": "ministryplatform-fetch",
    "homepage": "https://github.com/perimeterchurch/MPFetch",
    "private": false,
    "version": "1.0.0",
    "description": "A library for fetching data from MinistryPlatform using the customwidget API",
    "keywords": [
        "ministryplatform",
        "customwidget",
        "fetch",
        "api"
    ],
    "license": "MIT",
    "type": "module",
    "files": [
        "./dist"
    ],
    "main": "./dist/mpfetch.umd.cjs",
    "module": "./dist/mpfetch.js",
    "types": "./dist/mpfetch.d.ts",
    "exports": {
        "types": "./dist/mpfetch.d.ts",
        "import": "./dist/mpfetch.js",
        "require": "./dist/mpfetch.umd.cjs"
    },
    "scripts": {
        "dev": "vite --port 5500",
        "check-types": "tsc --noEmit",
        "build": "vite build",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "eslint": "^9.21.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.2",
        "typescript": "~5.7.3",
        "typescript-eslint": "^8.25.0",
        "vite": "^6.2.0",
        "vite-plugin-dts": "^4.5.0"
    },
    "packageManager": "pnpm@9.15.5"
}
