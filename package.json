{
    "name": "ministryplatform-fetch",
    "homepage": "https://github.com/perimeterchurch/MPFetch",
    "private": false,
    "version": "1.1.0",
    "description": "A library for fetching data from MinistryPlatform using the customwidget API",
    "keywords": [
        "ministryplatform",
        "customwidget",
        "fetch",
        "api"
    ],
    "license": "MIT",
    "type": "module",
    "files": [
        "./dist"
    ],
    "main": "./dist/mpfetch.umd.cjs",
    "module": "./dist/mpfetch.js",
    "types": "./dist/mpfetch.d.ts",
    "exports": {
        "types": "./dist/mpfetch.d.ts",
        "import": "./dist/mpfetch.js",
        "require": "./dist/mpfetch.umd.cjs"
    },
    "scripts": {
        "dev": "vite --port 5500",
        "check-types": "tsc --noEmit",
        "build": "vite build",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.29.1",
        "vite": "^6.2.5",
        "vite-plugin-dts": "^4.5.3"
    }
}
